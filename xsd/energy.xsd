						<complexType>
							<sequence minOccurs="0">
								<element ref="energy:SurfaceComponent"/>
							</sequence>
							<attributeGroup ref="gml:AssociationAttributeGroup"/>
						</complexType>
					</element>
					<element name="correspondsTo" type="bldg:BoundarySurfacePropertyType" minOccurs="0"/>
				</sequence>
			</extension>
		</complexContent>
