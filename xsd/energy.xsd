					<element name="normalIncidenceEmittance" type="energy:EmittancePropertyType" minOccurs="0">
						<annotation>
							<documentation>Emittance in surface normal direction</documentation>
						</annotation>
					</element>
					<element name="numberOfPanes" type="integer" minOccurs="0">
						<annotation>
							<documentation>Number of glazing panes</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="GlazingPropertyType">
		<sequence minOccurs="0">
			<element ref="energy:Glazing"/>
		</sequence>
		<attributeGroup ref="gml:AssociationAttributeGroup"/>
	</complexType>
	<element name="MechanicalVentilation" type="energy:MechanicalVentilationType" substitutionGroup="energy:EnergyConversionSystem"/>
	<complexType name="MechanicalVentilationType">
		<complexContent>
			<extension base="energy:EnergyConversionSystemType">
				<sequence>
					<element name="heatRecovery" type="boolean"/>
					<element name="recuperationFactor"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="MechanicalVentilationPropertyType">
		<sequence minOccurs="0">
			<element ref="energy:MechanicalVentilation"/>
		</sequence>
		<attributeGroup ref="gml:AssociationAttributeGroup"/>
	</complexType>
	<element name="ThermalDistributionSystem" type="energy:ThermalDistributionSystemType" substitutionGroup="energy:EnergyDistributionSystem"/>
	<complexType name="ThermalDistributionSystemType">
		<complexContent>
			<extension base="energy:EnergyDistributionSystemType">
				<sequence>
					<element name="isCirculation" type="boolean" minOccurs="0"/>
					<element name="medium" type="gml:CodeType" minOccurs="0"/>
					<element name="nominalFlow" minOccurs="0"/>
					<element name="returnTemperature" minOccurs="0"/>
					<element name="supplyTemperature" minOccurs="0"/>
					<element name="thermalLossesFactor" minOccurs="0"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="ThermalDistributionSystemPropertyType">
		<sequence minOccurs="0">
			<element ref="energy:ThermalDistributionSystem"/>
		</sequence>
		<attributeGroup ref="gml:AssociationAttributeGroup"/>
	</complexType>
	<element name="ElectricalResistance" type="energy:ElectricalResistanceType" substitutionGroup="energy:EnergyConversionSystem"/>
	<complexType name="ElectricalResistanceType">
		<complexContent>
			<extension base="energy:EnergyConversionSystemType">
				<sequence/>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="ElectricalResistancePropertyType">
		<sequence minOccurs="0">
			<element ref="energy:ElectricalResistance"/>
		</sequence>
		<attributeGroup ref="gml:AssociationAttributeGroup"/>
	</complexType>
	<element name="ThermalZone" type="energy:ThermalZoneType" substitutionGroup="core:_CityObject">
		<annotation>
			<documentation>A thermal zone is a zone of a building which serves as unit for the building heating and cooling simulation. For the simulation, a thermal zone is considered as isothermal.
It is a semantic object, which may be or not related to a geometric entity.</documentation>
		</annotation>
	</element>
	<complexType name="ThermalZoneType">
		<complexContent>
			<extension base="core:AbstractCityObjectType">
				<sequence>
					<element name="additionalThermalBridgeUValue" minOccurs="0">
						<annotation>
							<documentation>Transmission heat loss contribution [W/m².K] of the thermal bridges of the thermal zone shell to the overall UValue.</documentation>
						</annotation>
					</element>
					<element name="cooledFloorArea" minOccurs="0">
						<annotation>
							<documentation>Actively cooled floor area [m²]</documentation>
						</annotation>
					</element>
					<element name="effectiveThermalCapacity" minOccurs="0">
						<annotation>
							<documentation>Thermal capacity [Wh/K] of the zone including the contributing construction layers and zone fournitures.</documentation>
						</annotation>
					</element>
					<element name="heatedFloorArea" minOccurs="0">
						<annotation>
							<documentation>Actively heated floor area [m²]</documentation>
						</annotation>
					</element>
					<element name="indirectlyHeatedAreaRatio" minOccurs="0">
						<annotation>
							<documentation>[find precise definition]</documentation>
						</annotation>
					</element>
					<element name="infiltrationRate" minOccurs="0">
						<annotation>
							<documentation>Air change rate [1/h] due to air leakage of the zone shell.</documentation>
						</annotation>
					</element>
					<element name="relates" type="energy:UsageZonePropertyType" minOccurs="0" maxOccurs="unbounded"/>
          			<element maxOccurs="unbounded" name="boundedBy" type="energy:ThermalBoundarySurfacePropertyType"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="ThermalZonePropertyType">
		<sequence minOccurs="0">
			<element ref="energy:ThermalZone"/>
		</sequence>
		<attributeGroup ref="gml:AssociationAttributeGroup"/>
	</complexType>
	<element name="HeatPump" type="energy:HeatPumpType" substitutionGroup="energy:EnergyConversionSystem"/>
	<complexType name="HeatPumpType">
		<complexContent>
			<extension base="energy:EnergyConversionSystemType">
				<sequence>
					<element name="carnotEfficiency" type="double" minOccurs="0"/>
					<element name="heatSource" type="gml:CodeType"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="HeatPumpPropertyType">
		<sequence minOccurs="0">
			<element ref="energy:HeatPump"/>
		</sequence>
		<attributeGroup ref="gml:AssociationAttributeGroup"/>
	</complexType>
	<element name="ThermalStorageSystem" type="energy:ThermalStorageSystemType" substitutionGroup="energy:_StorageSystem"/>
	<complexType name="ThermalStorageSystemType">
		<complexContent>
			<extension base="energy:_StorageSystemType">
				<sequence>
					<element name="averageTemperature" minOccurs="0"/>
					<element name="medium" type="gml:CodeType" minOccurs="0"/>
					<element name="thermalLossesFactor" minOccurs="0">
						<annotation>
							<documentation>U*A</documentation>
						</annotation>
					</element>
					<element name="volume" minOccurs="0"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="ThermalStorageSystemPropertyType">
		<sequence minOccurs="0">
			<element ref="energy:ThermalStorageSystem"/>
		</sequence>
		<attributeGroup ref="gml:AssociationAttributeGroup"/>
	</complexType>
	<element name="ScheduleLoD3" type="energy:ScheduleLoD3Type" substitutionGroup="energy:_Schedule"/>
	<complexType name="ScheduleLoD3Type">
		<complexContent>
			<extension base="energy:_ScheduleType">
				<sequence>
					<element name="timeDependingValues" type="energy:_TimeSeriesPropertyType"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="ScheduleLoD3PropertyType">
		<sequence>
			<element ref="energy:ScheduleLoD3"/>
		</sequence>
	</complexType>
	<element name="ShadingType" type="energy:ShadingTypeType" substitutionGroup="gml:_Object"/>
	<complexType name="ShadingTypeType">
		<sequence>
			<element name="maximumCoverRatio">
				<annotation>
					<documentation>Maximum fraction of the related opening which can be covered with the shading device.</documentation>
				</annotation>
			</element>
			<element name="name" type="string"/>
			<element name="transmittance" type="energy:TransmittancePropertyType"/>
		</sequence>
	</complexType>
	<complexType name="ShadingTypePropertyType">
		<sequence>
			<element ref="energy:ShadingType"/>
		</sequence>
	</complexType>
	<element name="DailySchedule" type="energy:DailyScheduleType" substitutionGroup="gml:_Object"/>
	<complexType name="DailyScheduleType">
		<sequence>
			<element name="dayType" type="gml:CodeType"/>
			<element name="schedule" type="energy:_TimeSeriesPropertyType"/>
      		<element name="scheduleLoD2" type="energy:ScheduleLoD2PropertyType"/>
		</sequence>
	</complexType>
	<complexType name="DailySchedulePropertyType">
		<sequence>
			<element ref="energy:DailySchedule"/>
		</sequence>
	</complexType>
	<element name="BuildingUnit" type="energy:BuildingUnitType" substitutionGroup="core:_CityObject">
		<annotation>
			<documentation>A building unit is a part of usage zone, such as dwelling or workplace, which is related to a single occupant entity.</documentation>
		</annotation>
	</element>
	<complexType name="BuildingUnitType">
		<complexContent>
			<extension base="core:AbstractCityObjectType">
				<sequence>
					<element name="numberOfRooms" type="integer" minOccurs="0"/>
					<element name="ownerName" type="string" minOccurs="0"/>
					<element name="ownershipType" type="gml:CodeType" minOccurs="0"/>
					<element name="usedArea" minOccurs="0"/>
					<element name="occupiedBy" type="energy:OccupancyPropertyType" minOccurs="0" maxOccurs="unbounded"/>
					<element name="has" type="energy:FacilitiesPropertyType" minOccurs="0" maxOccurs="unbounded"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="BuildingUnitPropertyType">
		<sequence minOccurs="0">
			<element ref="energy:BuildingUnit"/>
		</sequence>
		<attributeGroup ref="gml:AssociationAttributeGroup"/>
	</complexType>
	<element name="ScheduleLoD2" type="energy:ScheduleLoD2Type" substitutionGroup="energy:_Schedule"/>
	<complexType name="ScheduleLoD2Type">
		<complexContent>
			<extension base="energy:_ScheduleType">
		        <sequence>
		          <element maxOccurs="unbounded" name="dailySchedule" type="energy:DailySchedulePropertyType"/>
		        </sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="ScheduleLoD2PropertyType">
		<sequence>
			<element ref="energy:ScheduleLoD2"/>
		</sequence>
	</complexType>
	<element name="_TimeSeries" type="energy:_TimeSeriesType" abstract="true" substitutionGroup="gml:_Object">
		<annotation>
			<documentation>Consistent series of time-depending values.</documentation>
		</annotation>
	</element>
	<complexType name="_TimeSeriesType" abstract="true">
		<sequence>
			<element name="id" type="string"/>
			<element name="temporalExtent"/>
			<element name="variableProperties" type="energy:TimeValuesPropertiesPropertyType" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="_TimeSeriesPropertyType">
		<sequence>
			<element ref="energy:_TimeSeries"/>
		</sequence>
	</complexType>
	<simpleType name="ThermalBoundarySurfaceTypeType">
		<restriction base="string">
			<enumeration value="OuterWall"/>
			<enumeration value="SharedWall"/>
			<enumeration value="FlatRoof"/>
			<enumeration value="PitchedRoof"/>
			<enumeration value="CellarCeiling"/>
			<enumeration value="BasementFloor"/>
			<enumeration value="TopCeiling">
				<annotation>
					<documentation>Ceiling of the highest storey of the zone.</documentation>
				</annotation>
			</enumeration>
		</restriction>
	</simpleType>
	<simpleType name="SurfaceSideType">
		<restriction base="string">
			<enumeration value="Inside"/>
			<enumeration value="Outside"/>
		</restriction>
	</simpleType>
</schema>
