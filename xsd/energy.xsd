			<extension base="core:AbstractCityObjectType">
				<sequence>
					<element name="coolingSchedule" type="energy:_SchedulePropertyType" minOccurs="0">
						<annotation>
							<documentation>Temperature schedule [°C] of the active cooling inside the usage zone.</documentation>
						</annotation>
					</element>
					<element name="heatingSchedule" type="energy:_SchedulePropertyType" minOccurs="0">
						<annotation>
							<documentation>Temperature schedule [°C] of the active heating inside the usage zone.</documentation>
						</annotation>
					</element>
					<element name="usageZoneClass" type="gml:CodeType">
						<annotation>
							<documentation>Usage class of the zone.</documentation>
						</annotation>
					</element>
					<element name="usedArea">
						<annotation>
							<documentation>Used floor area [m²].</documentation>
						</annotation>
					</element>
					<element name="usedFloorNumbers" type="integer" minOccurs="0" maxOccurs="unbounded">
						<annotation>
							<documentation>List of the number of floors part of the usage zone (0 = ground level).</documentation>
						</annotation>
					</element>
					<element name="ventilationSchedule" type="energy:_SchedulePropertyType" minOccurs="0">
						<annotation>
							<documentation>Air change rate [1/h] schedule of the ventilation system inside the usage zone.</documentation>
						</annotation>
					</element>
					<element name="contains" type="energy:BuildingUnitPropertyType" minOccurs="0" maxOccurs="unbounded"/>
					<element name="occupiedBy" type="energy:OccupancyPropertyType" minOccurs="0" maxOccurs="unbounded"/>
					<element name="has" type="energy:FacilitiesPropertyType" minOccurs="0" maxOccurs="unbounded"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="UsageZonePropertyType">
